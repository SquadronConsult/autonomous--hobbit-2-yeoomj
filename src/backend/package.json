{
  "name": "agricultural-management-system-backend",
  "version": "1.0.0",
  "description": "Backend service for the Containerized Autonomous Agricultural Management System",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/cors": "8.3.0",
    "@fastify/swagger": "8.3.1",
    "@minio/minio-js": "7.1.1",
    "@opentelemetry/api": "1.4.1",
    "@opentelemetry/instrumentation": "0.41.1",
    "@opentelemetry/sdk-node": "0.41.1",
    "@turf/helpers": "6.5.0",
    "bcrypt": "5.1.0",
    "class-sanitizer": "1.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express-async-handler": "1.2.0",
    "express-rate-limit": "6.7.0",
    "express-validator": "7.0.1",
    "fastify": "4.17.0",
    "geometry_msgs": "4.2.3",
    "helmet": "7.0.0",
    "http-status-codes": "2.2.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.0",
    "keycloak-connect": "21.1.1",
    "opossum": "6.0.0",
    "performance-now": "2.1.0",
    "pg": "8.11.0",
    "pino": "8.14.1",
    "rate-limiter-flexible": "2.4.1",
    "rclnodejs": "0.22.3",
    "retry": "0.13.1",
    "rxjs": "7.8.0",
    "std_msgs": "4.2.3",
    "tsyringe": "4.7.0",
    "uuid": "9.0.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "29.0.0",
    "@jest/types": "29.5.0",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.16",
    "@types/pg": "8.10.2",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "k6": "0.45.0",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "pactum": "3.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "supertest": "6.3.3",
    "testcontainers": "9.1.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}